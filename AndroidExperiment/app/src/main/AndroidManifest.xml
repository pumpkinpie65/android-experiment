<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pumpkinpie65.androidexperiment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".dashboard.DashboardActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".modernfeed.ModernFeedActivity"
            android:label="@string/modern_feed_title"
            android:parentActivityName=".dashboard.DashboardActivity"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".recyclerview.RecyclerViewActivity"
            android:label="@string/recyclerview_title"
            android:parentActivityName=".dashboard.DashboardActivity" />

        <activity
            android:name=".structsandalgos.StructsAndAlgosActivity"
            android:label="@string/structs_and_algos_title" />

    </application>

</manifest>